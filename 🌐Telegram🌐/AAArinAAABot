import telebot
import config
import random

from telebot import types
from random import randint

bot= telebot.TeleBot(config.TOKEN)

@bot.message_handler(commands= ['start'])
def welcome (message):
    sticker= open('sticker.webp', 'rb')
    bot.send_sticker(message.chat.id, sticker)

    markup= types.ReplyKeyboardMarkup(resize_keyboard=True)
    item1= types.KeyboardButton('üêà–†–∞—Å—Å–∫–∞–∂–∏ –ø—Ä–æ –∫–æ—Ç–∏–∫–æ–≤!üêà')
    item2= types.KeyboardButton('üê∏–†–∞—Å—Å–∫–∞–∂–∏ –ø—Ä–æ –ª—è–≥—É—à–µ–∫!üê∏')
    item3 = types.KeyboardButton('üê∏–í–∏–¥–µ–æ —Å –ª—è–≥—É—à–∫–æ–πüê∏')

    markup.add(item1, item2, item3)

    bot.send_message(message.chat.id, "–ü—Ä–∏–≤–µ—Ç, –¥–æ—Ä–æ–≥–æ–π –¥—Ä—É–≥!\n–Ø - –ö–æ—Ç–µ–Ω–æ–∫, –±–æ—Ç, —Å–æ–∑–¥–∞–Ω–Ω—ã–π, —á—Ç–æ–±—ã —Ä–∞—Å—Å–∫–∞–∑–∞—Ç—å —Ç–µ–±–µ –Ω–µ–º–Ω–æ–≥–æ –∏–Ω—Ç–µ—Ä–µ—Å–Ω–æ–≥–æ!".format(bot.get_me()),
                    parse_mode='html', reply_markup=markup)

@bot.message_handler(content_types =['text'])
def lalala (message):
    if message.chat.type=='private':
        if message.text == 'üêà–†–∞—Å—Å–∫–∞–∂–∏ –ø—Ä–æ –∫–æ—Ç–∏–∫–æ–≤!üêà':
            rand_n = randint(1,5)
            if rand_n==1:
                photo1= open('photo1.jpg', 'rb')
                bot.send_photo(message.chat.id, photo1, "–Ø —Ä–∞—Å—Å–∫–∞–∂—É –ø—Ä–æ –∫–æ—à–∫—É –ø–æ—Ä–æ–¥—ã <b>–¢—É—Ä–µ—Ü–∫–∞—è –∞–Ω–≥–æ—Ä–∞</b>. "
                                                  "–≠—Ç–æ—Ç –ø—Ä–µ–∫—Ä–∞—Å–Ω—ã–π –∫–æ—Ç–∏–∫ –æ—Ç–Ω–æ—Å–∏—Ç—Å—è –∫ –ø–æ–ª—É–¥–ª–∏–Ω–Ω–æ—à–µ—Ä—Å—Ç–Ω—ã–º. "
                                                  "–î–∞–Ω–Ω–∞—è –ø–æ—Ä–æ–¥–∞ —Å—Ñ–æ—Ä–º–∏—Ä–æ–≤–∞–ª–∞—Å—å –Ω–∞ —Ç–µ—Ä—Ä–∏—Ç–æ—Ä–∏–∏ –¢—É—Ä—Ü–∏–∏ –∏, –≤–æ–∑–º–æ–∂–Ω–æ, –ø—Ä–æ–∏–∑–æ—à–ª–∞ –æ—Ç —Å–∫—Ä–µ—â–∏–≤–∞–Ω–∏—è –¥–æ–º–∞—à–Ω–µ–π –º–µ—Å—Ç–Ω–æ–π –∏ –∫–∞–≤–∫–∞–∑—Å–∫–æ–π –¥–∏–∫–æ–π(–ª–µ—Å–Ω–æ–π). "
                                                  "–°–≤–æ–µ –Ω–∞–∑–≤–∞–Ω–∏–µ –ø–æ—Ä–æ–¥–∞ —É–Ω–∞—Å–ª–µ–¥–æ–≤–∞–ª–∞ –æ—Ç –≥.–ê–Ω–≥–æ—Ä–∞. –Ω—ã–Ω–µ-–ê–Ω–∫–∞—Ä–∞. "
                                                  "–ß–∞—â–µ –≤—Å–µ–≥–æ –≤—Å—Ç—Ä–µ—á–∞—é—Ç—Å—è –∫–æ—à–∫–∏ –±–µ–ª–æ–≥–æ —Ü–≤–µ—Ç–∞, –Ω–æ —Ç–∞–∫–∂–µ –º–æ–∂–Ω–æ –≤—Å—Ç—Ä–µ—Ç–∏—Ç—å –∏ –¥—Ä—É–≥–∏–µ –æ–∫—Ä–∞—Å—ã, –∫–æ—Ç–æ—Ä—ã–µ –ø–æ–ø—É–ª—Ä–Ω—ã –≤ –ê–º–µ—Ä–∏–∫–µ. "
                                                  "–≠—Ç–∏ –∫—Ä–∞—Å–∏–≤—ã–µ –∫–æ—à–∫–∏ –æ–±–ª–∞–¥–∞—é—Ç –Ω–µ–∂–Ω—ã–º –∏ –ª–∞—Å–∫–æ–≤—ã–º —Ö–∞—Ä–∞–∫—Ç–µ—Ä–æ–º, –∞ —Ç–∞–∫–∂–µ –æ—á–µ–Ω—å –ø—Ä–∏–≤—è–∑–∞–Ω—ã –∫ —Ö–æ–∑—è–µ–≤–∞–º. "
                                                  "–û–Ω–∏ –ø–æ–Ω—è—Ç–ª–∏–≤—ã. –±—ã—Å—Ç—Ä–æ –≤–æ—Å–ø—Ä–∏–Ω–∏–º–∞—é—Ç –∏ —É—Å–≤–∞–∏–≤–∞—é—Ç –∂–µ–ª–∞–Ω–∏—è –∏ —Ç—Ä–µ–±–æ–≤–∞–Ω–∏—è —á–µ–ª–æ–≤–µ–∫–∞. "
                                                  "–í–º–µ—Å—Ç–µ —Å —ç—Ç–∏–º –∏–º —Å–≤–æ–π—Å—Ç–≤–µ–Ω–Ω—ã —Ñ–ª–µ–≥–º–∞—Ç–∏—á–Ω–æ—Å—Ç—å –∏ –Ω–µ–∫–æ—Ç–æ—Ä–∞—è –º–µ–¥–ª–∏—Ç–µ–ª—å–Ω–æ—Å—Ç—å",parse_mode='html')
            elif rand_n==2:
                photo2= open('photo2.webp', 'rb')
                bot.send_photo(message.chat.id, photo2, "–ù–∞ —Ñ–æ—Ç–æ - –ø—Ä–µ–∫—Ä–∞—Å–Ω–∞—è –∫–æ—à–∫–∞ –ø–æ—Ä–æ–¥—ã <b>–®–∞–Ω—Ç–∏–ª—å–∏-—Ç–∏—Ñ—Ñ–∞–Ω–∏</b>. "
                                                        "–û—á–µ–Ω—å —Ä–µ–¥–∫–∞—è –ø–æ—Ä–æ–¥–∞, –ø–æ–ø—É–ª—è—Ä–Ω–∞—è –∏—Å–∫–ª—é—á–∏—Ç–µ–ª—å–Ω–æ —Å—Ä–µ–¥–∏ –ª—é–±–∏—Ç–µ–ª–µ–π ¬´–ø—É—à–∏—Å—Ç–∏–∫–æ–≤¬ª. "
                                                        "–ü—Ä–∏–º–µ—á–∞—Ç–µ–ª—å–Ω–∞ —Å–≤–æ–∏–º —à–æ–∫–æ–ª–∞–¥–Ω—ã–º –æ–∫—Ä–∞—Å–æ–º –∏ —è–Ω—Ç–∞—Ä–Ω—ã–º–∏ –≥–ª–∞–∑–∞–º–∏. "
                                                        "–ú–∞–∫—Å–∏–º–∞–ª—å–Ω–∞—è –¥–ª–∏–Ω–∞ —à—É–±–∫–∏ –¥–æ—Å—Ç–∏–≥–∞–µ—Ç—Å—è –≤ —Ä–∞–π–æ–Ω–µ –ª–∞–ø –∏ –≥—Ä—É–¥–∏.", parse_mode='html')
            elif rand_n ==3:
                photo3 = open('photo3.jpg', 'rb')
                bot.send_photo(message.chat.id, photo3, "–ù–∞ —Ñ–æ—Ç–æ - –ø—Ä–µ–∫—Ä–∞—Å–Ω–∞—è –∫–æ—à–∫–∞ –ø–æ—Ä–æ–¥—ã <b>–ë—Ä–∏—Ç–∞–Ω—Å–∫–∞—è –∫–æ—Ä–æ—Ç–∫–æ—à–µ—Ä—Å—Ç–Ω–∞—è</b>. "
                                                        "–ë—Ä–∏—Ç–∞–Ω—Å–∫–∏–µ –∫–æ—Ä–æ—Ç–∫–æ—à–µ—Ä—Å—Ç–Ω—ã–µ –∏ –¥–ª–∏–Ω–Ω–æ—à–µ—Ä—Å—Ç–Ω—ã–µ –∫–æ—à–∫–∏ –º–∏—Ä–æ–ª—é–±–∏–≤—ã –∏ –ª–∞—Å–∫–æ–≤—ã. "
                                                        "–û–Ω–∏ –Ω–µ–∑–∞–≤–∏—Å–∏–º—ã, —Ç–µ—Ä–ø–µ–ª–∏–≤—ã –∏ —Å–ø–æ–∫–æ–π–Ω—ã, —Ö–æ—Ä–æ—à–æ –ª–∞–¥—è—Ç —Å –¥–µ—Ç—å–º–∏ –∏ –∂–∏–≤–æ—Ç–Ω—ã–º–∏. "
                                                        "–ò–≥—Ä–∏–≤—ã, –Ω–æ –Ω–µ –≥–∏–ø–µ—Ä–∞–∫—Ç–∏–≤–Ω—ã. –û–Ω–∏ –Ω–µ –æ—á–µ–Ω—å —Ä—É—á–Ω—ã–µ, –æ–±—ã—á–Ω–æ –ø—Ä–µ–¥–ø–æ—á–∏—Ç–∞—é—Ç —Å–∏–¥–µ—Ç—å —Ä—è–¥–æ–º —Å —á–µ–ª–æ–≤–µ–∫–æ–º, –Ω–µ–∂–µ–ª–∏ –≤—Å–µ –≤—Ä–µ–º—è –ª–µ–∑—Ç—å –∫ –Ω–µ–º—É –Ω–∞ –∫–æ–ª–µ–Ω–∏. "
                                                        "–ù–µ –ª—é–±—è—Ç, –∫–æ–≥–¥–∞ –∏—Ö –Ω–∞—Å–∏–ª—å–Ω–æ –ø—ã—Ç–∞—é—Ç—Å—è —Ç–∏—Å–∫–∞—Ç—å. "
                                                        "–í –æ–±—â–µ–º, —ç—Ç–æ –Ω–∞—Å—Ç–æ—è—â–∏–µ –∞—Ä–∏—Å—Ç–æ–∫—Ä–∞—Ç—ã —Å—Ä–µ–¥–∏ –¥–æ–º–∞—à–Ω–∏—Ö –∫–æ—à–µ–∫, –∑–Ω–∞–º–µ–Ω–∏—Ç–∞—è ¬´–±—Ä–∏—Ç–∞–Ω—Å–∫–∞—è —Å–¥–µ—Ä–∂–∞–Ω–Ω–æ—Å—Ç—å¬ª ‚Äî —ç—Ç–æ –ø—Ä–æ –Ω–∏—Ö.", parse_mode='html')
            elif rand_n ==4:
                photo4 = open('photo4.jpeg', 'rb')
                bot.send_photo(message.chat.id, photo4, "–†–∞—Å—Å–∫–∞–∂—É-–∫–∞ —è –ø—Ä–æ –∫–æ—Ç–∏–∫–æ–≤ <b>–°–∏–∞–º—Å–∫–æ–π</b> –ø–æ—Ä–æ–¥—ã. "
                                                        "–°–∏–∞–º—Å–∫–∞—è –∫–æ—à–∫–∞ –∏–º–µ–µ—Ç –≤–µ—Å—å–º–∞ —Ö–∞—Ä–∞–∫—Ç–µ—Ä–Ω—É—é –≤–Ω–µ—à–Ω–æ—Å—Ç—å, –æ—Ç–ª–∏—á–∏—Ç–µ–ª—å–Ω—ã–º–∏ —á–µ—Ä—Ç–∞–º–∏ –∫–æ—Ç–æ—Ä–æ–π —è–≤–ª—è—é—Ç—Å—è —Ç–æ–Ω–∫–æ–µ, –¥–ª–∏–Ω–Ω–æ–µ —Ç–µ–ª–æ, –≥–æ–ª–æ–≤–∞ –≤ –≤–∏–¥–µ –¥–ª–∏–Ω–Ω–æ–≥–æ –∫–ª–∏–Ω–∞, –±–æ–ª—å—à–∏–µ –≥–ª–∞–∑–∞ —è—Ä–∫–æ-—Å–∏–Ω–µ–≥–æ —Ü–≤–µ—Ç–∞, –±–æ–ª—å—à–∏–µ —É—à–∏. "
                                                        "–®–µ—Ä—Å—Ç—å –∫–æ—Ä–æ—Ç–∫–∞—è, –ø–ª–æ—Ç–Ω–æ –ø—Ä–∏–ª–µ–≥–∞—é—â–∞—è –∫ —Ç–µ–ª—É, –±–µ–∑ –ø–æ–¥—à–µ—Ä—Å—Ç–∫–∞. "
                                                        "–û—á–µ–Ω—å –¥–ª–∏–Ω–Ω—ã–π —Ö–ª—ã—Å—Ç–æ–æ–±—Ä–∞–∑–Ω—ã–π —Ö–≤–æ—Å—Ç, —Ç–æ–Ω–∫–∏–π –æ—Ç —Å–∞–º–æ–≥–æ –æ—Å–Ω–æ–≤–∞–Ω–∏—è —Å –∑–∞–æ—Å—Ç—Ä–µ–Ω–Ω—ã–º –¥–ª–∏–Ω–Ω—ã–º –∫–æ–Ω—á–∏–∫–æ–º. "
                                                        "–î–ª—è —Å–∏–∞–º—Å–∫–∏—Ö –∫–æ—à–µ–∫ —Ö–∞—Ä–∞–∫—Ç–µ—Ä–µ–Ω –æ–∫—Ä–∞—Å –∫–æ–ª–æ—Ä-–ø–æ–∏–Ω—Ç (—Å–≤–µ—Ç–ª–∞—è —à–µ—Ä—Å—Ç—å —Å –±–æ–ª–µ–µ —Ç—ë–º–Ω—ã–º –æ–∫—Ä–∞—Å–æ–º –Ω–∞ –ª–∞–ø–∞—Ö, –º–æ—Ä–¥–µ, —É—à–∞—Ö –∏ —Ö–≤–æ—Å—Ç–µ). "
                                                        "–¢–∞–∫–æ–π –æ–∫—Ä–∞—Å ‚Äî —ç—Ç–æ –ø—Ä–æ—è–≤–ª–µ–Ω–∏–µ –Ω–µ–ø–æ–ª–Ω–æ–≥–æ –∞–ª—å–±–∏–Ω–∏–∑–º–∞, –Ω–∞–∑—ã–≤–∞–µ–º–æ–≥–æ –∞–∫—Ä–æ–º–µ–ª–∞–Ω–∏–∑–º. "
                                                        "–î–µ–π—Å—Ç–≤–∏–µ –∞–∫—Ä–æ–º–µ–ª–∞–Ω–∏–∑–º–∞ —Å–≤—è–∑–∞–Ω–æ —Å —Ç–µ–º–ø–µ—Ä–∞—Ç—É—Ä–Ω—ã–º–∏ –æ—Å–æ–±–µ–Ω–Ω–æ—Å—Ç—è–º–∏ –∂–∏–≤–æ–≥–æ –æ—Ä–≥–∞–Ω–∏–∑–º–∞: –≤ —Ç–µ–ø–ª—ã—Ö —á–∞—Å—Ç—è—Ö —Ç–µ–ª–∞ –≤—ã—Ä–∞–±–∞—Ç—ã–≤–∞–µ—Ç—Å—è –º–µ–Ω—å—à–µ –ø–∏–≥–º–µ–Ω—Ç–∞, —á–µ–º –≤ —Ö–æ–ª–æ–¥–Ω—ã—Ö, –ø–æ—ç—Ç–æ–º—É —É—à–∏, –ª–∞–ø—ã, —Ö–≤–æ—Å—Ç –∏ –º–æ—Ä–¥–∞ –æ–∫—Ä–∞—à–µ–Ω—ã –≤ –±–æ–ª–µ–µ —Ç—ë–º–Ω—ã–π –ø–æ —Å—Ä–∞–≤–Ω–µ–Ω–∏—é —Å –æ—Å—Ç–∞–ª—å–Ω—ã–º —Ç–µ–ª–æ–º —Ü–≤–µ—Ç. "
                                                        "–° –≤–æ–∑—Ä–∞—Å—Ç–æ–º –∫–æ–Ω—Ç—Ä–∞—Å—Ç –ø–æ–∏–Ω—Ç–æ–≤ –ø–æ —Å—Ä–∞–≤–Ω–µ–Ω–∏—é —Å —Ç–µ–ª–æ–º –º–æ–∂–µ—Ç —Å—Ç–∞—Ç—å –º–µ–Ω—å—à–µ. "
                                                        "–ü–æ—Å–ª–µ –≥–æ–¥–∞ –±–æ–ª—å—à–∏–Ω—Å—Ç–≤–æ –∫–æ—à–µ–∫ –ø–æ–∏–Ω—Ç–æ–≤–æ–≥–æ –æ–∫—Ä–∞—Å–∞ –∏–º–µ—é—Ç –∑–∞—Ç–µ–º–Ω–µ–Ω–∏–µ –ø–æ –∫–æ—Ä–ø—É—Å—É. "
                                                        "–°–∏–∞–º—Å–∫–∏–µ –∫–æ—Ç—è—Ç–∞ —Ä–æ–∂–¥–∞—é—Ç—Å—è –∞–±—Å–æ–ª—é—Ç–Ω–æ –±–µ–ª—ã–º–∏, —á–µ—Ä–µ–∑ –Ω–µ—Å–∫–æ–ª—å–∫–æ –¥–Ω–µ–π –ø–æ—Å–ª–µ —Ä–æ–∂–¥–µ–Ω–∏—è –Ω–∞—á–∏–Ω–∞—é—Ç —Ç–µ–º–Ω–µ—Ç—å –ø–æ–∏–Ω—Ç—ã, –æ–∫–æ–Ω—á–∞—Ç–µ–ª—å–Ω–æ –æ–∫—Ä–∞—Å —É—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ—Ç—Å—è –≤ 5‚Äî10 –º–µ—Å—è—Ü–µ–≤.", parse_mode='html')
            elif rand_n ==5:
                photo5 = open('photo5.jpg', 'rb')
                bot.send_photo(message.chat.id, photo5, "–†–∞—Å—Å–∫–∞–∂—É-–∫–∞ —è –ø—Ä–æ –º–∏–ª–µ–π—à–∏—Ö –∫–æ—Ç–∏–∫–æ–≤ –ø–æ—Ä–æ–¥—ã <b>–ú–∞–Ω—á–∫–∏–Ω</b>. "
                                                        "–ú–∞–Ω—á–∫–∏–Ω ‚Äî –ø–æ—Ä–æ–¥–∞ –∫–æ—à–µ–∫, –∫–æ—Ç–æ—Ä—É—é –Ω–µ –≤—ã–≤–æ–¥–∏–ª–∏ —Å–ø–µ—Ü–∏–∞–ª—å–Ω–æ . "
                                                        "–ö —ç—Ç–æ–π –ø–æ—Ä–æ–¥–µ –æ—Ç–Ω–æ—Å—è—Ç—Å—è –∫–æ—à–∫–∏ ‚Äî –Ω–æ—Å–∏—Ç–µ–ª–∏ –æ—Å–æ–±–æ–≥–æ –≥–µ–Ω–∞ –∞—Ö–æ–Ω–¥—Ä–æ–ø–ª–∞–∑–∏–∏, –∏–∑-–∑–∞ –∫–æ—Ç–æ—Ä–æ–≥–æ —É –Ω–∏—Ö –∫–æ—Ä–æ—Ç–∫–∏–µ –∫–æ–Ω–µ—á–Ω–æ—Å—Ç–∏. "
                                                        "–ü–æ—Ä–æ–¥–∞ –±–µ—Ä–µ—Ç –Ω–∞—á–∞–ª–æ –æ—Ç –æ–¥–Ω–æ–π-–µ–¥–∏–Ω—Å—Ç–≤–µ–Ω–Ω–æ–π –∫–æ—à–∫–∏ –ø–æ –∫–ª–∏—á–∫–µ –ï–∂–µ–≤–∏—á–∫–∞, –∫–æ—Ç–æ—Ä—É—é –Ω–∞—à–ª–∞ –∏ –≤—ã—Ö–æ–¥–∏–ª–∞ –∞–º–µ—Ä–∏–∫–∞–Ω—Å–∫–∞—è –∑–∞–≤–æ–¥—á–∏—Ü–∞. " 
                                                        "–ö–æ—Ä–æ—Ç–∫–∏–µ –ª–∞–ø–∫–∏ –ø–∏—Ç–æ–º–∏—Ü—ã –ø–µ—Ä–µ–¥–∞–ª–∏—Å—å –∏ –µ–µ –∫–æ—Ç—è—Ç–∞–º.", parse_mode='html')
        if message.text == 'üê∏–†–∞—Å—Å–∫–∞–∂–∏ –ø—Ä–æ –ª—è–≥—É—à–µ–∫!üê∏':
            rand_num = randint(1, 5)
            if rand_num==1:
                frog1= open('frog1.jpeg', 'rb')
                bot.send_photo(message.chat.id, frog1, "<b>–õ—è–≥—É—à–∫–∞-–≥–æ–ª–∏–∞—Ñ</b>. "
                                                  "–≠—Ç–æ —Å–∞–º–∞—è –±–æ–ª—å—à–∞—è –≤ –º–∏—Ä–µ –ª—è–≥—É—à–∫–∞! "
                                                  "–î–ª–∏–Ω–∞ —Ç–µ–ª–∞ –±–µ–∑ –ª–∞–ø –º–æ–∂–µ—Ç –¥–æ—Å—Ç–∏–≥–∞—Ç—å 32—Å–º, –∞ —Å –≤—ã—Ç—è–Ω—É—Ç—ã–º–∏ –∑–∞–¥–Ω–∏–º–∏ –ª–∞–ø–∫–∞–º–∏ -81-90—Å–º. "
                                                  "–ú–∞—Å—Å–∞ —ç—Ç–∏—Ö –ª—è–≥—É—à–µ–∫ –¥–æ—Å—Ç–∏–≥–∞–µ—Ç 3,3 –∫–≥!. "
                                                  "–û–Ω–∏ –∂–∏–≤—É—Ç –≤ –≤–æ–¥–æ–ø–∞–¥–∞—Ö –±—ã—Å—Ç—Ä—ã—Ö –∏ —Ç—Ä–æ–ø–∏—á–µ—Å–∫–∏—Ö —Ä–µ–∫. "
                                                  "–î–ª–∏–Ω–∞ –ø—Ä—ã–∂–∫–∞ —Å–æ—Å—Ç–∞–≤–ª—è–µ—Ç 3-3,5 –º–µ—Ç—Ä–∞. –ü–æ—ç—Ç–æ–º—É –ø—Ä–∏ –º–∞–ª–µ–π—à–µ–π –æ–ø–∞—Å–Ω–æ—Å—Ç–∏ —ç—Ç–∏ –ª—è–≥—É—à–∫–∏ –±—ã—Å—Ç—Ä–æ —Å–∫—Ä—ã–≤–∞—é—Ç—Å—è –≤ –±–µ–∑–æ–ø–∞—Å–Ω–æ–º –º–µ—Å—Ç–µ. "
                                                  "–ü–æ–¥–æ–±—Ä–∞—Ç—å—Å—è –∫ –Ω–∏–º –æ—á–µ–Ω—å —Ç—Ä—É–¥–Ω–æ, –≤–µ–¥—å –æ–Ω–∏ –æ–±–ª–∞–¥–∞—é—Ç –ø—Ä–µ–≤–æ—Å—Ö–æ–¥–Ω—ã–º –∑—Ä–µ–Ω–∏–µ–º –∏ –º–æ–≥—É—Ç –∑–∞–º–µ—Ç–∏—Ç—å –¥–≤–∏–∂–µ–Ω–∏–µ –Ω–∞ —Ä–∞—Å—Å—Ç–æ—è–Ω–∏–∏ 40 –º–µ—Ç—Ä–æ–≤. "
